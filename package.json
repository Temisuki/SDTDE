{
  "name": "electron-angular-webpack",
  "version": "1.0.0",
  "description": "Simple editor for 7 days to die .xml files",
  "keywords": [
    "electron",
    "angular",
    "webpack",
    "boilerplate",
    "live reload",
    "typescript",
    "sass"
  ],
  "author": "Raborh & Temisuki",
  "license": "GPL v3",
  "main": "electron.js",
  "scripts": {
    "dev": "tsc electron.ts --outDir . && cross-env NODE_ENV=dev webpack-dev-server --port=4200",
    "prod": "webpack && electron dist/electron.js",
    "packager:win": "cross-env NODE_OS=win node builder/packager.js",
    "packager:mac": "cross-env NODE_OS=mac node builder/packager.js",
    "packager:linux": "cross-env NODE_OS=linux node builder/packager.js",
    "installer:win": "npm run packager:win && cross-env NODE_OS=win node builder/installer.js",
    "installer:mac": "npm run packager:mac && cross-env NODE_OS=mac node builder/installer.js",
    "installer:linux": "npm run packager:linux && cross-env NODE_OS=linux node builder/installer.js"
  },
  "devDependencies": {
    "@types/xml2js": "^0.4.3",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron-packager": "^12.1.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^5.3.1",
    "img-loader": "^3.0.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.3",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "underscore-template-loader": "^1.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "optionalDependencies": {
    "appdmg": "^0.5.2",
    "electron-installer-debian": "^0.8.1",
    "electron-installer-dmg": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.6.4"
  },
  "dependencies": {
    "@angular/cdk": "^6.4.0",
    "@angular/animations": "^4.0.1",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/http": "^6.0.9",
    "@angular/material": "^6.4.0",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/router": "^6.0.9",
    "@types/xml2json": "^0.10.0",
    "angular-in-memory-web-api": "^0.6.0",
    "autoprefixer": "^9.0.0",
    "core-js": "^2.5.7",
    "electron": "^2.0.5",
    "ngx-electron": "^1.0.4",
    "rxjs": "^6.2.2",
    "xml2js": "^0.4.19",
    "zone.js": "^0.8.26"
  }
}
